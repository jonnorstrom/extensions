{
  "manifest_version": 2,
  "name": "Drumpfify",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-310min.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [ "http://*.google.com/", "http://*.com/", "tabs" ]
}
